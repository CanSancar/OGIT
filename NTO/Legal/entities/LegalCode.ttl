@prefix rdfs:    	<http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     	<http://www.w3.org/2002/07/owl#> .
@prefix dcterms: 	<http://purl.org/dc/terms/> .
@prefix ogit:    	<http://www.purl.org/ogit/> .
@prefix ogit.Legal: <http://www.purl.org/ogit/Legal/> .

ogit.Legal:LegalCode
	a rdfs:Class;
	rdfs:subClassOf ogit:Entity;
	rdfs:label "LegalCode";
	dcterms:description """A body of primary legislation or laws that have been enacted by the legislative body
	(such as a national parliament or state legislature). It provides the broad framework of rules within a specific
	legal area (e.g., civil law, criminal law)""";
	# For ranges, see http://dublincore.org/documents/dcmi-period/
	dcterms:valid "start=2025-04-09;";
	dcterms:creator "Daniel Maier";
	ogit:scope "NTO";
	ogit:parent ogit:Node;
	ogit:mandatory-attributes (
		ogit:name
		ogit:id
	);
	ogit:optional-attributes (
		ogit.Legal:juristictionCode
		ogit:content
		ogit.Legal:footnote
		ogit:language
		ogit:createdAt
	);
	ogit:indexed-attributes (
	);
	ogit:allowed (
			[ ogit:contains  ogit.Legal:LegalNorm ]
			[ ogit:contains  ogit.Legal:Attachment ]
			[ ogit.Legal:empowers  ogit.Organization ]
	);
.