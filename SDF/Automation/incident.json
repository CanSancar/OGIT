{
  "sdf": {
    "id": "incident",
    "entities": {
      "incident": {
        "type": "ogit/Automation/AutomationIssue",
        "presence": "root",
        "attributes": {
          "ogit/Automation/originNode": "originNode",
          "ogit/Automation/issueType": "issueType",
          "ogit/status": "hiroStatus",
          "ogit/subject": "subject",
          "/assignedGroup": "assignee",
          "/description": "description",
          "/incidentStatus": "sourceStatus",
          "/openedAt": "openedAt",
          "/priority": "priority",
          "/reportedAt": "reportedAt",
          "/summary": "summary",
          "/impact": "impact",
          "/urgency": "urgency",
          "ogit/Automation/deployStatus": "closeNotes"
        },
        "constants": {
          "/actual_type": "IncidentType"
        },
        "initializers": {
          "/ProcessIssue": "process_me"
        },
        "volatile-attributes": [
          "hiroStatus"
        ]
      },
      "worklog": {
        "type": "ogit/Timeseries",
        "presence": "optional",
        "cache-size": 5,
        "attributes": {
          "$timeseries": "worklog"
        },
        "constants": {
          "ogit/name": "History",
          "/TimeseriesType": "Issue History"
        }
      },
      "comments": {
        "type": "ogit/Timeseries",
        "presence": "optional",
        "cache-size": 5,
        "cache-target": "parent",
        "cache-attribute": "/response",
        "attributes": {
          "$timeseries": "comments"
        },
        "constants": {
          "ogit/name": "UserComment",
          "/TimeseriesType": "UserComment"
        }
      }
    },
    "verbs": [
      {
        "from": "incident",
        "to": "worklog",
        "id": "ogit/generates"
      },
      {
        "from": "incident",
        "to": "comments",
        "id": "ogit/generates"
      }
    ]
  }
}